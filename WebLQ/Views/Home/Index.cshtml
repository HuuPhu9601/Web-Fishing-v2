    <script>
        var isPercentage = true;
        var prizes = [
            {
                //text: "Đá Quý X2",
                img: "images/da_quy_x2.png",
                number: 1, // 1%,
                percentpage: 0.05 // 1%
            },
            {
                text: "Tulen Thiên Sứ STL-79",
                img: "images/tulen_thien_su_stl-79.png",
                number: 1,
                percentpage: 0.05 // 5%
            },
            {
                text: "Krixi Nữ Hoàng Thiên Nga",
                img: "images/krixi_nuhoangthiennga.png",
                number: 1,
                percentpage: 0.1 // 10%
            },
            {
                text: "Bright Toshiro Hitsugaya",
                img: "images/bright_toshi_hitsugaya.png",
                number: 1,
                percentpage: 0.2 // 24%
            },
            {
                text: "Ngộ Không Nhóc Tỳ Bá Đạo",
                img: "images/ngo_khong_nhoc_ty_ba_dao.png",
                percentpage: 0.6 // 60%
            },
            {
                text: "Violet Tiệc Bãi Biển",
                img: "images/violet_tiec_bai_bien.png",
                percentpage: 0.1 // 60%
            },
            {
                text: "Grack Tiệc Bãi Biển",
                img: "images/grack_tiec_bai_bien.png",
                percentpage: 0.6 // 60%
            },
            {
                text: "Raz Siêu Cấp Tin Tặc",
                img: "images/raz_sieu_cap_tin_tac.png",
                percentpage: 0.6 // 60%
            },
            {
                text: "Lữ Bố Ichigo Kurosaki",
                img: "images/lubo_ichigo_kurosaki.png",
                percentpage: 0.6 // 60%
            },
            {
                text: "Murad Byakuya Kuchiki",
                img: "images/murad_byakuya_kuchiki.png",
                percentpage: 0.6 // 60%
            },
        ];
        document.addEventListener(
            "DOMContentLoaded",
            function () {
                hcLuckywheel.init({
                    id: "luckywheel",
                    config: function (callback) {
                        callback &&
                            callback(prizes);
                    },
                    mode: "both",
                    getPrize: function (callback) {
                        var rand = randomIndex(prizes);
                        var chances = rand;
                        callback && callback([rand, chances]);
                    },
                    gotBack: function (data) {
                        if (data == null) {
                            Swal.fire(
                                'Chương trình kết thúc',
                                'Đã hết phần thưởng',
                                'error'
                            )
                        } else if (data == 'Chúc bạn may mắn lần sau') {
                            Swal.fire(
                                'Bạn không trúng thưởng',
                                data,
                                'error'
                            )
                        } else {
                            Swal.fire(
                                'Đã trúng giải',
                                data,
                                'success'
                            )
                        }
                    }
                });
            },
            false
        );
        function randomIndex(prizes) {
            if (isPercentage) {
                var counter = 1;
                for (let i = 0; i < prizes.length; i++) {
                    if (prizes[i].number == 0) {
                        counter++
                    }
                }
                if (counter == prizes.length) {
                    return null
                }
                let rand = Math.random();
                let prizeIndex = null;
                console.log(rand);
                switch (true) {
                    case rand < prizes[9].percentpage:
                        prizeIndex = 9;
                        break;
                    case rand < prizes[9].percentpage + prizes[8].percentpage:
                        prizeIndex = 8;
                        break;
                    case rand < prizes[9].percentpage + prizes[8].percentpage + prizes[7].percentpage:
                        prizeIndex = 7;
                        break;
                    case rand < prizes[9].percentpage + prizes[8].percentpage + prizes[7].percentpage + prizes[6].percentpage:
                        prizeIndex = 6;
                        break;
                    case rand < prizes[9].percentpage + prizes[8].percentpage + prizes[7].percentpage + prizes[6].percentpage + prizes[5].percentpage:
                        prizeIndex = 5;
                        break;
                    case rand < prizes[9].percentpage + prizes[8].percentpage + prizes[7].percentpage + prizes[6].percentpage + prizes[5].percentpage + prizes[4].percentpage:
                        prizeIndex = 4;
                        break;
                    case rand < prizes[9].percentpage + prizes[8].percentpage + prizes[7].percentpage + prizes[6].percentpage + prizes[5].percentpage + prizes[4].percentpage + prizes[3].percentpage:
                        prizeIndex = 3;
                        break;
                    case rand < prizes[9].percentpage + prizes[8].percentpage + prizes[7].percentpage + prizes[6].percentpage + prizes[5].percentpage + prizes[4].percentpage + prizes[3].percentpage + prizes[2].percentpage:
                        prizeIndex = 2;
                        break;
                    case rand < prizes[9].percentpage + prizes[8].percentpage + prizes[7].percentpage + prizes[6].percentpage + prizes[5].percentpage + prizes[4].percentpage + prizes[3].percentpage + prizes[2].percentpage + prizes[1].percentpage:
                        prizeIndex = 1;
                        break;
                    case rand < prizes[9].percentpage + prizes[8].percentpage + prizes[7].percentpage + prizes[6].percentpage + prizes[5].percentpage + prizes[4].percentpage + prizes[3].percentpage + prizes[2].percentpage + prizes[1].percentpage + prizes[0].percentpage:
                        prizeIndex = 0;
                        break;
                }
                if (prizes[prizeIndex].number != 0) {
                    prizes[prizeIndex].number = prizes[prizeIndex].number - 1
                    return prizeIndex
                } else {
                    return randomIndex(prizes)
                }
            } else {
                var counter = 0;
                for (let i = 0; i < prizes.length; i++) {
                    if (prizes[i].number == 0) {
                        counter++
                    }
                }
                if (counter == prizes.length) {
                    return null
                }
                var rand = (Math.random() * (prizes.length)) >>> 0;
                if (prizes[rand].number != 0) {
                    prizes[rand].number = prizes[rand].number - 1
                    return rand
                } else {
                    return randomIndex(prizes)
                }
            }
        }
    </script>

